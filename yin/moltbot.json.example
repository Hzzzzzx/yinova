{
  "meta": {
    "lastTouchedVersion": "2026.1.27-beta.1"
  },
  "browser": {
    "enabled": false,
    "defaultProfile": "openclaw",
    "profiles": {
      "openclaw": {
        "cdpPort": 18860,
        "color": "#1A1A1A"
      }
    }
  },
  "auth": {
    "profiles": {
      "zai:default": {
        "provider": "zai",
        "mode": "api_key"
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "zai/glm-5",
        "fallbacks": ["zai/glm-4.7"]
      },
      "workspace": "__YINOVA_ROOT__/yin/workspace",
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      }
    }
  },
  "tools": {
    "web": {
      "search": { "enabled": true, "apiKey": "" },
      "fetch": { "enabled": true }
    }
  },
  "channels": { "telegram": { "enabled": false } },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "loopback",
    "auth": {
      "mode": "token",
      "token": "replace_with_random_secret_token"
    },
    "tailscale": { "mode": "off" },
    "http": {
      "endpoints": {
        "chatCompletions": { "enabled": true }
      }
    }
  },
  "models": {
    "providers": {
      "zai": {
        "baseUrl": "https://open.bigmodel.cn/api/coding/paas/v4",
        "models": [
          {"id": "glm-5", "name": "GLM-5"},
          {"id": "glm-4.7", "name": "GLM-4.7"},
          {"id": "glm-4.7-flash", "name": "GLM-4.7-Flash"}
        ]
      }
    }
  },
  "skills": { "install": { "nodeManager": "npm" } },
  "plugins": { "entries": { "telegram": { "enabled": false } } }
}
